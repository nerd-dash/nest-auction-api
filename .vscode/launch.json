{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch auction-api",
            "request": "launch",
            "type": "node",
            "runtimeExecutable": "npx",
            "runtimeArgs": [
                "nx",
                "serve",
                "auction-api"
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/apps/auction-api"
        },
        {
          "name": "Launch auction-api-e2e",
          "request": "launch",
          "type": "node",
          "runtimeExecutable": "npx",
          "runtimeArgs": [
              "nx",
              "run",
              "auction-api-e2e:e2e"
          ],
          "console": "integratedTerminal",
          "cwd": "${workspaceFolder}"
      }
    ]
}
